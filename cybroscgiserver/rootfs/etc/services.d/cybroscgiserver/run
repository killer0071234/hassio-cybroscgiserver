#!/command/with-contenv bashio
# ==============================================================================
# Home Assistant Community Add-on: CybroScgiServer
# Runs CybroScgiServer
# ==============================================================================
version=`sed -n 's/^APP_VERSION = //p' /usr/local/bin/scgi_server/src/constants.py`
bashio::log.info "Starting CybroScgiServer $version..."
cd /usr/local/bin/scgi_server/src
exec python3 app.py
